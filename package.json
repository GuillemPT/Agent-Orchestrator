{
  "name": "agent-orchestrator",
  "version": "1.0.0",
  "description": "DevTools Architect - Agent Orchestrator for AI Development Environments",
  "main": "dist/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:renderer": "vite",
    "dev:main": "NODE_ENV=development node scripts/build-main.mjs && wait-on http://localhost:3000 && NODE_ENV=development electron .",
    "dev:main:watch": "concurrently \"node scripts/build-main.mjs --watch\" \"electron .\"",
    "build": "npm run build:renderer && npm run build:main",
    "build:renderer": "vite build",
    "build:main": "node scripts/build-main.mjs",
    "start": "electron .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\""
  },
  "keywords": [
    "electron",
    "react",
    "agent",
    "orchestrator",
    "copilot",
    "mcp"
  ],
  "author": "GuillemPT",
  "license": "MIT",
  "dependencies": {
    "chokidar": "^3.5.3",
    "keytar": "^7.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^4.0.18",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "esbuild": "^0.27.3",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "fast-glob": "^3.3.3",
    "jsdom": "^27.0.1",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.4"
  }
}
